<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ring_shape"
        android:width="300dp"
        android:height="300dp"
        android:gravity="center"/>
    <item android:drawable="@drawable/rectangle"
        android:width="10dp"
        android:height="20dp"
        android:gravity="center_horizontal"/>
    <item android:drawable="@drawable/rectangle"
        android:width="10dp"
        android:height="20dp"
        android:gravity="bottom|center_horizontal"/>
    <item android:drawable="@drawable/rectangle"
        android:width="20dp"
        android:height="10dp"
        android:gravity="center_vertical"/>
    <item android:drawable="@drawable/rectangle"
        android:width="20dp"
        android:height="10dp"
        android:gravity="right|center_vertical"/>
    <item
        android:width="20dp"
        android:height="10dp"
        android:top="41dp"
        android:start="48dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/black"/>
            </shape>
        </rotate>
    </item>
    <item
        android:width="20dp"
        android:height="10dp"
        android:bottom="40dp"
        android:start="43dp"
        android:gravity="bottom">
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/black"/>
            </shape>
        </rotate>
    </item>
    <item
        android:width="20dp"
        android:height="10dp"
        android:top="50dp"
        android:end="23dp"
        android:gravity="right">
        <rotate
            android:fromDegrees="135"
            android:toDegrees="135"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/black"/>
            </shape>
        </rotate>
    </item>
    <item
        android:width="20dp"
        android:height="10dp"
        android:bottom="55dp"
        android:end="38dp"
        android:gravity="right|bottom">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/black"/>
            </shape>
        </rotate>
    </item>

